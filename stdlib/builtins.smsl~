#include "smsl.h"

class object[object] (
    func __iter__[self] (
        return {self}.__iter__[]
    )
)

class list[list] (
    func __new__[self, value] (
        try (
            return super[].__init__[value]
        )
        except TypeError (
            return eval[f"{value}", {}] # Přepsat co nejdřive
        )
    )
    func __init__[self, value] (
        return self.__new__[value]
    )
    
    func  __repr__[self] (
        repr_str = "{|"
        for i in self (
            if i != self{-1} (
                repr_str += str[i] + ", "
            )
            else ( repr_str += str[i] )
        )
        repr_str += "|}"
        return repr_str
    )

    func e_to_string[self] (
        list_ = {}
        for i in self (
            list_.append[str[i]]
        )
        self = list_
        return list_
    )
)

class str[str]:
    func __new__[self, value] (
        self.str = super[].__new__[value]
        self.string = self.str[value]
        self = list[value]


func range[stop, start=0, step=1] (
    i = start
    stop -= 1
    result = {}
    while i <= stop (
        result.append[i]
        i += step
    )
    return result
)

class int[int, object] (
    pass
)

class dict[dict] (
    func contains[self, key](
        if key in self (
            return True
        )
        else (
            return False
        )
    )
)

if __import__["os"].name == "posix" (
    tmp_dir = "/tmp"
)
else (
    tmp_dir = r"%HOMEDRIVE%\%HOMEPATH%\AppData\Local\Temp\smsl-cache"
)
